{
  "name": "textual-velocity",
  "version": "0.7.0",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": "textual-velocity:start-session"
  },
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "dependencies": {
    "baconjs": "0.7.84",
    "bplist": "0.0.4",
    "chokidar": "1.0.6",
    "classnames": "2.2.5",
    "fs-plus": "2.9.1",
    "fs-xattr": "0.1.14",
    "lodash.debounce": "4.0.6",
    "moment": "2.13.0",
    "ramda": "0.21.0",
    "react-for-atom": "15.0.2-1",
    "scandal": "2.2.0",
    "sifter": "0.5.1"
  },
  "devDependencies": {
    "standard": "7.1.2",
    "temp": "0.8.3"
  },
  "configSchema": {
    "path": {
      "description": "Path to folder where to find notes. Can be an absolute path or a relative path to `~/.atom` (defaults to `~/.atom/notes`). You'll need to (re)start the session if you change this path.",
      "type": "string",
      "default": ""
    },
    "sortField": {
      "type": "string",
      "default": "last_updated_at",
      "enum": [
        "title",
        "last_updated_at",
        "created_at"
      ]
    },
    "sortDirection": {
      "type": "string",
      "default": "desc",
      "enum": [
        "asc",
        "desc"
      ]
    },
    "listHeight": {
      "type": "number",
      "default": 150,
      "minimum": 0
    },
    "rowHeight": {
      "type": "number",
      "default": 20,
      "minimum": 8,
      "maximum": 80
    },
    "enableDeveloperConsoleLog": {
      "description": "Output to the developer console.",
      "type": "boolean",
      "default": false
    }
  },
  "standard": {
    "globals": [
      "afterEach",
      "atom",
      "beforeAll",
      "beforeEach",
      "describe",
      "Event",
      "expect",
      "HTMLElement",
      "it",
      "jasmine",
      "spyOn",
      "runs",
      "xdescribe",
      "waitsFor",
      "waitsForPromise"
    ]
  },
  "scripts": {
    "lint": "standard 'lib/**/*.js' && standard 'spec/**/*.js'"
  }
}
