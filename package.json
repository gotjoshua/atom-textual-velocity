{
  "name": "textual-velocity",
  "version": "0.7.0",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": "textual-velocity:start-session"
  },
  "engines": {
    "atom": ">=1.10.0 <2.0.0"
  },
  "providedServices": {
     "textual-velocity": {
      "description": "Exposes API to register columns and file read/writers",
      "versions": {
        "0.1.0": "provideServiceV0"
      }
    }
  },
  "dependencies": {
    "baconjs": "0.7.87",
    "chokidar": "1.6.0",
    "classnames": "2.2.5",
    "fs-plus": "2.9.1",
    "lodash.debounce": "4.0.8",
    "moment": "2.15.0",
    "ramda": "0.22.1",
    "react-for-atom": "15.3.1-0",
    "sifter": "0.5.2"
  },
  "optionalDependencies": {
    "bplist": "0.0.4",
    "fs-xattr": "0.1.14"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-plugin-syntax-flow": "6.13.0",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "eslint": "3.5.0",
    "eslint-config-standard": "6.0.1",
    "eslint-config-standard-jsx": "3.0.1",
    "eslint-plugin-flowtype": "2.18.2",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-standard": "2.0.0",
    "flow-bin": "0.32.0",
    "temp": "0.8.3"
  },
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint 'lib/**/*.js' && eslint 'spec/**/*.js'",
    "test": "apm test && npm run flow && npm run lint"
  }
}
