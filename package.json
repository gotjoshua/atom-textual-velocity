{
  "name": "textual-velocity",
  "version": "0.7.0",
  "repository": "https://github.com/viddo/atom-textual-velocity",
  "description": "Your mental notes at your fingertips!",
  "keywords": [
    "note",
    "notetaking",
    "notational velocity",
    "nvalt",
    "search",
    "filter",
    "wiki"
  ],
  "license": "MIT",
  "main": "./lib/main.js",
  "activationCommands": {
    "atom-workspace": "textual-velocity:start-session"
  },
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "dependencies": {
    "baconjs": "0.7.83",
    "bplist": "0.0.4",
    "chokidar": "1.0.6",
    "classnames": "2.2.3",
    "fs-plus": "2.8.1",
    "fs-xattr": "0.1.11",
    "lodash.debounce": "4.0.3",
    "moment": "2.11.2",
    "ramda": "0.19.1",
    "react-for-atom": "0.14.7-0",
    "scandal": "2.2.0",
    "sifter": "0.4.5"
  },
  "devDependencies": {
    "temp": "0.8.3"
  },
  "configSchema": {
    "panelHeight": {
      "type": "number",
      "default": 150,
      "minimum": 0
    },
    "path": {
      "description": "Path to folder where to find notes. Can be an absolute path or a relative path to `~/.atom` (defaults to `~/.atom/notes`). You'll need to (re)start the session if you change this path.",
      "type": "string",
      "default": ""
    },
    "sortField": {
      "type": "string",
      "default": "mtimestamp",
      "enum": ["name", "tags", "mtimestamp", "birthtimestamp"]
    },
    "sortDirection": {
      "type": "string",
      "default": "desc",
      "enum": ["asc", "desc"]
    }
  }
}
